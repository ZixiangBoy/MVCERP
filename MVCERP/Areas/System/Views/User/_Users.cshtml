@model PetaPoco.Page<DbEntity.t_user>

@{
    Layout = null;
}

    <table id="tableuser" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>用户名</th>
                <th>密码</th>
                <th>创建人</th>
                <th>创建时间</th>
                <th>启用</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Items == null || Model.Items.Count < 1) {
                <tr>
                    <td class="col-sm-5">没有您查询数据</td>
                </tr>
            }
            @foreach (var user in Model.Items) {
                <tr>
                    <td>@Html.DisplayFor(m => user.UserName)</td>
                    <td>@Html.DisplayFor(m => user.Pwd)</td>
                    <td>@Html.DisplayFor(m => user.Creator)</td>
                    <td>@Html.DisplayFor(m => user.CreateDate)</td>
                    <td>@Html.CheckBoxFor(m => user.IsUsing)</td>
                </tr>
            }
        </tbody>
    </table>

    @Html.Partial("~/Views/Shared/_Pager.cshtml", Model.Pager)

